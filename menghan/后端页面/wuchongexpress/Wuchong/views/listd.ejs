<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>Document</title>
    <style type="text/css">
        * {
            margin: 0;
            padding: 0;
        }
        #foot {
            border-collapse: collapse;
            width: 1200px;
            margin: 0 auto;
            text-align: center;
            margin-top: 30%;
        }
        table {
            border-collapse: collapse;
            width: 1000px;
            margin: 0 auto;
            text-align: center;
            display: block;
            margin-top: 100px;
        }
        table td, table th {
            color: #666;
            height: 45px;
        }
        table thead th {
            background-color: #fff;
            width: 220px;
        }
        #action{
            background-color: #CCE8EB;
            border: 1px solid #eee;
            width: 260px;    
        }
        table tr {
            background: #fff;
        }


        .container {
            margin-top: 80px;
        }
        
    </style> 
</head>
<body>
    
    <div class="container">
            
        <table  class="table">
            <thead>
                <tr>
                    <th>用户ID</th>
                    <th>打卡ID</th>
                    <th>打卡名名</th>
                    <th>打卡次数</th>
                    <th>提醒时间</th>
                    <th></th>
                    <th id="action">编辑</th>
                    <th></th>
                    <th id="action">删除</th>
                </tr>
            </thead>

            <tbody id="result"></tbody>
            <%for(var i=0;i < List.length;i++){%>
                <tr>
                    <td>
                        <%= List[i]["uID"]%>
                    </td>
                    <td>
                        <%= List[i]["dakaID"]%>
                    </td>
                    <td>
                        <%= List[i]["dname"]%>
                    </td>
                    <td>
                        <%= List[i]["dnum"]%>
                    </td>
                    <td>
                    <%= List[i]["dtime"]%>
                    </td><td></td>
                    <td>
                    <a href="/do?chapterid=<%= List[i]["dakaID"]%>">
                        编辑
                    </a>
                    </td><td></td>
                    <td>
                            <a href="/del?chapterid=<%= List[i]["dakaID"]%>">
                                删除
                            </a>
                    </td>
                </tr>
            <%}%>

        </table>
        <div id="foot">
            <p style="float: left;"onclick="showUser()">返回上一页</p>
            <p style="float: right;">确认</p>
        </div>
        
    </div>
    
        
</body>
<script>
    function showUser() {
            window.location = "http://localhost:3000/userguanli";
        }
    </script>
</html>
